{
    "name": "peresmishnyk/laravel-glide",
    "description": "Glide image transform for laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Michkire Dmytro",
            "email": "michkire@gmail.com",
            "homepage": "https://peresmishnyk.com"
        }
    ],
    "homepage": "https://github.com/peresmishnyk/laravel-glide",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "LaravelGlide"
    ],
    "require": {
        "illuminate/support": "~7|~8",
        "backpack/crud": "^4.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Peresmishnyk\\LaravelGlide\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Peresmishnyk\\LaravelGlide\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Peresmishnyk\\LaravelGlide\\AddonServiceProvider"
            ]
        }
    }
}
